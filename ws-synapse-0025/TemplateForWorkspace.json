{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"workspaceName": {
			"type": "string",
			"metadata": "Workspace name",
			"defaultValue": "ws-synapse-0025"
		},
		"ws-synapse-0025-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ws-synapse-0025-WorkspaceDefaultSqlServer'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:ws-synapse-0025.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"ws-synapse-0025-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://defaultsynapseadlsgen2.dfs.core.windows.net"
		}
	},
	"variables": {
		"workspaceId": "[concat('Microsoft.Synapse/workspaces/', parameters('workspaceName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('workspaceName'), '/ws-synapse-0025-WorkspaceDefaultSqlServer')]",
			"type": "Microsoft.Synapse/workspaces/linkedServices",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"parameters": {
					"DBName": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "AzureSqlDW",
				"typeProperties": {
					"connectionString": "[parameters('ws-synapse-0025-WorkspaceDefaultSqlServer_connectionString')]"
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('workspaceName'), '/ws-synapse-0025-WorkspaceDefaultStorage')]",
			"type": "Microsoft.Synapse/workspaces/linkedServices",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('ws-synapse-0025-WorkspaceDefaultStorage_properties_typeProperties_url')]"
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('workspaceName'), '/AutoResolveIntegrationRuntime')]",
			"type": "Microsoft.Synapse/workspaces/integrationRuntimes",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 0
						}
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/WorkspaceSystemIdentity')]",
			"type": "Microsoft.Synapse/workspaces/credentials",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"type": "ManagedIdentity",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/Create Schema')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "CREATE schema gold",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/External table creation')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "-- Create a database scoped credential\nCREATE DATABASE SCOPED CREDENTIAL AppCred\nWITH \nIDENTITY = 'Managed Identity'\n---Creating External Data source\nCREATE EXTERNAL DATA SOURCE SOURCE_SILVER\nWITH\n(\n    LOCATION = 'https://2025adls001.dfs.core.windows.net/silver',\n    CREDENTIAL = AppCred\n)\n\n\nCREATE EXTERNAL FILE FORMAT file_format_name\nWITH (\n        FORMAT_TYPE = PARQUET,\n        DATA_COMPRESSION = 'org.apache.hadoop.io.compress.SnappyCodec'\n        \n     )\n\nCREATE EXTERNAL DATA SOURCE SOURCE_GOLD\nWITH\n(\n    LOCATION = 'https://2025adls001.dfs.core.windows.net/gold',\n    CREDENTIAL = AppCred\n)\n\n\nCREATE EXTERNAL TABLE gold.extsales\nWITH(\n    LOCATION = 'extsales',\n    DATA_SOURCE = SOURCE_GOLD,\n    FILE_FORMAT = file_format_name\n) as \n\nselect * from gold.Sales\n\nselect * from gold.extsales",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/View Customer')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "CREATE view gold.Customer\nAS\n\nselect \n* \nfrom \nOPENROWSET(\n    BULK'https://2025adls001.blob.core.windows.net/silver/AdventureWorks_Customers/',\n    FORMAT='PARQUET'\n) AS QUERY2\n\n\nselect * from gold.Customer",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/View ProductCategories')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "CREATE view gold.ProductCategories\nAS\n\nselect \n* \nfrom \nOPENROWSET(\n    BULK'https://2025adls001.blob.core.windows.net/silver/AdventureWorks_ProductCategories/',\n    FORMAT='PARQUET'\n) AS QUERY3\nGO\n\nselect * from gold.Customer",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/View Products')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "CREATE view gold.Products\nAS\n\nselect \n* \nfrom \nOPENROWSET(\n    BULK'https://2025adls001.blob.core.windows.net/silver/AdventureWorks_Products/',\n    FORMAT='PARQUET'\n) AS QUERY4\nGO\n\nselect * from gold.Products",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/View Return')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "\n\ncreate view gold.Returns\nAS\n\nselect * from \nOPENROWSET(\n    BULK'https://2025adls001.dfs.core.windows.net/silver/AdventureWorks_Returns/',\n    FORMAT = 'parquet'\n)as query5\nGO\n\nselect * from gold.Returns\n\n\n\n\n\n\n\n\n\n\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/View Sales')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "\ncreate view gold.Sales\nAS\n\nselect * from \nOPENROWSET(\n    BULK'https://2025adls001.dfs.core.windows.net/silver/AdventureWorks_Sales/',\n    FORMAT = 'parquet'\n)as query5\nGO\n\nselect * from gold.Sales",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/View SubCategories')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "\nCREATE view gold.SubCategories\nAS\n\nselect \n* \nfrom \nOPENROWSET(\n    BULK'https://2025adls001.blob.core.windows.net/silver/AdventureWorks_SubCategories/',\n    FORMAT='PARQUET'\n) AS QUERY8\ngo\n\nselect * from gold.SubCategories",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/View Territories')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "create view gold.Territories\nAS\n\nselect * from \nOPENROWSET(\n    BULK'https://2025adls001.dfs.core.windows.net/silver/AdventureWorks_Territories/',\n    FORMAT = 'parquet'\n)as query9\nGO\n\nselect * from gold.Territories",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/calendar view')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "Create view gold.Calendar\nAS\n\nselect \n* \nfrom \nOPENROWSET(\n    BULK'https://2025adls001.dfs.core.windows.net/silver/AdventureWorks_Calendar/',\n    FORMAT='PARQUET'\n) AS QUERY1\n\n\nselect * from gold.Calendar\n\n\n\n\n\n\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "serverlessdatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		}
	]
}