		"LS_Sqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adfpractice"
		},
		"LS_Sqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Manikandan"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://practicestganshlamba001.dfs.core.windows.net/"
